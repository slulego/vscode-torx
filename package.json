{
	"name": "torx",
	"displayName": "Torx",
	"description": "Torx syntax and snippets.",
	"version": "0.0.1",
	"publisher": "slulego",
	"engines": {
		"vscode": "0.10.x"
	},
	"categories": [
		"Other",
		"Snippets"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/slulego/vscode-torx.git"
	},
	"icon": "icon.png",
	"galleryBanner": {
		"color": "#111111",
		"theme": "dark"
	},
	"scripts": {
		"update-grammar": "node ../../build/npm/update-grammar.js grammars/javascript.json ./syntaxes/torx.tmLanguage.json"
	},
	"contributes": {
		"languages": [
			{
				"id": "torx",
				"extensions": [
					".torx"
				],
				"aliases": [
					"Torx"
				],
				"mimetypes": [
					"text/html"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "torx",
				"scopeName": "text.html.torx",
				"path": "./syntaxes/torx.tmLanguage.json",
				"embeddedLanguages": {
					"section.embedded.source.torx": "torx"
				}
			}
		],
		"snippets": [
			{
				"language": "torx",
				"path": "./snippets/torx.snippets.json"
			}
		]
	},
	"__metadata": {
		"id": "c3f497bb-0a5f-4561-9fc5-b9195dd3ed8b",
		"publisherDisplayName": "Slulego",
